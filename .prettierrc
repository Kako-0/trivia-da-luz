{
	"useTabs": true,
	"singleQuote": false,
	"semi": true,
	"trailingComma": "none",
	"tabWidth": 2,
	"printWidth": 80,
	"bracketSameLine": true,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte",
				"svelteSortOrder": "options-scripts-markup-styles",
				"svelteIndentScriptAndStyle": true
			}
		}
	]
}
